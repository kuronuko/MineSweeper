<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="300sp"
    android:layout_height="300sp"
    android:padding="10sp"
    android:background="@drawable/masu_2"
    >
    
    <ImageButton
	    android:id="@+id/left_top"
	    android:layout_width="30sp"
	    android:layout_height="30sp"
	    android:layout_alignLeft="@+id/left"
	    android:layout_alignTop="@+id/top"
	    android:background="@drawable/one"
	    android:onClick="buttonMethod" />
    
    <ImageButton
        android:id="@+id/left"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/center"
        android:background="@drawable/two"
        android:onClick="buttonMethod" />

    <ImageButton
        android:id="@+id/left_bottom"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_alignLeft="@+id/left"
        android:layout_alignTop="@+id/bottom"
        android:background="@drawable/zero"
        android:onClick="buttonMethod" />
    
    <ImageButton
	    android:id="@+id/top"
	    android:layout_width="30sp"
	    android:layout_height="30sp"
	    android:layout_alignLeft="@+id/center"
	    android:layout_alignParentTop="true"
	    android:background="@drawable/batu"
	    android:onClick="buttonMethod" />
    
    <ImageButton
        android:id="@+id/seq3_2"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_above="@+id/seq3_3"
        android:layout_alignLeft="@+id/seq3_3"
        android:layout_marginBottom="12dp"
        android:background="@drawable/one"
        android:onClick="buttonMethod"
        android:padding="10sp" />
    
    <ImageButton
        android:id="@+id/seq3_3"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_above="@+id/center"
        android:layout_alignLeft="@+id/center"
        android:background="@drawable/one"
        android:onClick="buttonMethod"
        android:padding="10sp" />
    
    <ImageButton
	    android:id="@+id/center"
	    android:layout_width="30sp"
	    android:layout_height="30sp"
	    android:layout_margin="12sp"
	    android:layout_centerInParent="true"
	    android:onClick="buttonMethod"
	    android:background="@drawable/batu" />
    
    <ImageButton
        android:id="@+id/seq3_5"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_above="@+id/seq3_6"
        android:layout_alignLeft="@+id/seq3_6"
        android:layout_marginBottom="10dp"
        android:background="@drawable/one"
        android:onClick="buttonMethod"
        android:padding="10sp" />
    
    <ImageButton
        android:id="@+id/seq3_6"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_above="@+id/bottom"
        android:layout_alignLeft="@+id/bottom"
        android:layout_marginBottom="13dp"
        android:background="@drawable/batu"
        android:onClick="buttonMethod"
        android:padding="10sp" />

    <ImageButton
        android:id="@+id/bottom"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_alignLeft="@+id/center"
        android:layout_alignParentBottom="true"
        android:background="@drawable/one"
        android:onClick="buttonMethod" />
    
    <ImageButton
	    android:id="@+id/right_top"
	    android:layout_width="30sp"
	    android:layout_height="30sp"
	    android:layout_alignLeft="@+id/right"
	    android:layout_alignTop="@+id/top"
	    android:background="@drawable/one"
	    android:onClick="buttonMethod" />
    
    <ImageButton
        android:id="@+id/seq7_2"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_above="@+id/seq7_3"
        android:layout_alignLeft="@+id/seq7_3"
        android:layout_marginBottom="12dp"
        android:background="@drawable/batu"
        android:onClick="buttonMethod"
        android:padding="10sp" />
    
    <ImageButton
        android:id="@+id/seq7_3"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_above="@+id/right"
        android:layout_alignLeft="@+id/right"
        android:layout_marginBottom="12dp"
        android:background="@drawable/zero"
        android:onClick="buttonMethod"
        android:padding="10sp" />
    
    <ImageButton
	    android:id="@+id/right"
	    android:layout_width="30sp"
	    android:layout_height="30sp"
	    android:layout_alignParentRight="true"
	    android:layout_centerVertical="true"
	    android:background="@drawable/two"
	    android:onClick="buttonMethod" />
    
    <ImageButton
        android:id="@+id/seq7_5"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_above="@+id/seq7_6"
        android:layout_alignLeft="@+id/seq7_6"
        android:layout_marginBottom="10dp"
        android:background="@drawable/two"
        android:onClick="buttonMethod"
        android:padding="10sp" />
    
    <ImageButton
        android:id="@+id/seq7_6"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_above="@+id/right_bottom"
        android:layout_alignLeft="@+id/right_bottom"
        android:layout_marginBottom="14dp"
        android:background="@drawable/one"
        android:onClick="buttonMethod"
        android:padding="10sp" />
    
    <ImageButton
	    android:id="@+id/right_bottom"
	    android:layout_width="30sp"
	    android:layout_height="30sp"
	    android:layout_alignLeft="@+id/right"
	    android:layout_alignTop="@+id/bottom"
	    android:background="@drawable/one"
	    android:onClick="buttonMethod" />

</RelativeLayout>
